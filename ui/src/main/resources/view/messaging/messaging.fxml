<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
    </padding>
    <ListView fx:id="receive" focusTraversable="false" prefHeight="200.0" prefWidth="200.0" styleClass="chatlist" VBox.vgrow="ALWAYS">
        <VBox.margin>
            <Insets bottom="4.0"/>
        </VBox.margin>
    </ListView>
    <Label fx:id="notification" maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER"/>
    <TextField fx:id="send" promptText="Type a message" VBox.vgrow="NEVER">
        <VBox.margin>
            <Insets top="4.0"/>
        </VBox.margin>
    </TextField>
</VBox>
